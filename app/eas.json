{
  "cli": {
    "version": ">= 11.0.3"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "apk",
        "env": {
          "GOOGLE_SERVICES_JSON": "./google-services.json"
        }
      },
      "ios": {
        "env": {
          "GOOGLE_SERVICE_INFO_PLIST": "./GoogleService-Info.plist"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
